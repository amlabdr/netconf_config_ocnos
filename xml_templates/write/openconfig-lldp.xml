<?xml version='1.0' encoding='UTF-8'?>
<data xmlns="urn:ietf:params:xml:ns:netconf:base:1.0">
  <lldp xmlns="http://openconfig.net/yang/lldp">
    <config>
      <enabled>true</enabled>
      <hello-timer/>
      <suppress-tlv-advertisement>
        <!-- # entries: 0.. -->
      </suppress-tlv-advertisement>
      <system-name/>
      <system-description/>
      <chassis-id/>
      <chassis-id-type/>
    </config>
    <state>
      <enabled>true</enabled>
      <hello-timer/>
      <suppress-tlv-advertisement>
        <!-- # entries: 0.. -->
      </suppress-tlv-advertisement>
      <system-name/>
      <system-description/>
      <chassis-id/>
      <chassis-id-type/>
      <counters>
        <frame-in/>
        <frame-out/>
        <frame-error-in/>
        <frame-discard/>
        <tlv-discard/>
        <tlv-unknown/>
        <last-clear/>
        <tlv-accepted/>
        <entries-aged-out/>
      </counters>
    </state>
    <interfaces>
      <interface>
        <name/>
        <config>
          <name/>
          <enabled>true</enabled>
        </config>
        <state>
          <name/>
          <enabled>true</enabled>
          <counters>
            <frame-in/>
            <frame-out/>
            <frame-error-in/>
            <frame-discard/>
            <tlv-discard/>
            <tlv-unknown/>
            <last-clear/>
            <frame-error-out/>
          </counters>
        </state>
        <neighbors>
          <neighbor>
            <id/>
            <config/>
            <state>
              <system-name/>
              <system-description/>
              <chassis-id/>
              <chassis-id-type/>
              <id/>
              <age/>
              <last-update/>
              <ttl/>
              <port-id/>
              <port-id-type/>
              <port-description/>
              <management-address/>
              <management-address-type/>
            </state>
            <custom-tlvs>
              <tlv>
                <type/>
                <oui/>
                <oui-subtype/>
                <config/>
                <state>
                  <type/>
                  <oui/>
                  <oui-subtype/>
                  <value/>
                </state>
              </tlv>
            </custom-tlvs>
            <capabilities>
              <capability>
                <name/>
                <config/>
                <state>
                  <name/>
                  <enabled/>
                </state>
              </capability>
            </capabilities>
          </neighbor>
        </neighbors>
      </interface>
    </interfaces>
  </lldp>
</data>
